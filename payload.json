							"name": "Run KICS Scan",
							"uses": "Checkmarx/kics-github-action@v1.4",
							"with": {
								"ignore_on_exit": "results",
								"path": "./",
								"queries": "git::https://github.com/rafaela-soares/query,./"
							}
						}
					]
				}
