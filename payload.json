					"steps": [
						{
							"name": "Checkout",
							"uses": "actions/checkout@master"
						},
						{
							"env": {
								"CF_API_KEY": "${{ secrets.CF_API_KEY }}",
								"PIPELINE_NAME": "my_project/test2"
							},
							"id": "run-pipeline",
							"name": "run pipeline",
							"uses": "codefresh-io/codefresh-pipeline-runner@v5",
